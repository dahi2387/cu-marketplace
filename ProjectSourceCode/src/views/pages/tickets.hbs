<div class="mb-3" style="margin-left: 50px; margin-right: 50px;">
    {{> message}}

    <table class="table">
        <thead>
            <tr>
                <th scope="col">TicketID</th>
                <th scope="col">Ticket For Sale</th>
                <th scope="col">Email</th>
                <th scope="col">EventID</th>
            </tr>
        </thead>
        <tbody>
            {{#each tickets}}
                <tr>
                    <td>{{ticketid}}</td>
                    <td>{{isforsale}}</td>
                    <td>{{email}}</td>
                    <td>{{eventid}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
    <a href="/buy" class="button btn-primary" style="margin-top: 25px;">Back</a>
</div>